{
    "name": "cultuurnet/silex-service-provider-oauth",
    "description": "A Silex service provider for our OAuth component.",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [],
    "require": {
        "cultuurnet/symfony-security-oauth": "dev-feature/III-413",
        "cultuurnet/symfony-security-oauth-redis": "~0.1",
        "silex/silex": "~1.3",
        "cultuurnet/symfony-security-oauth-uitid": "~0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7",
        "squizlabs/php_codesniffer": "~2.3",
        "phing/phing": "~2.11",
        "satooshi/php-coveralls": "~0.7"
    },
    "autoload": {
        "psr-4": {
            "CultuurNet\\SilexServiceProviderOAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CultuurNet\\SilexServiceProviderOAuth\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}
